<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/DECORATION/1.6.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://maven.apache.org/DECORATION/1.6.0 http://maven.apache.org/xsd/decoration-1.6.0.xsd"
		name="DBFlute Hamcrest">
	<skin>
		<groupId>org.apache.maven.skins</groupId>
		<artifactId>maven-fluido-skin</artifactId>
		<version>1.4</version>
	</skin>

	<custom>
		<fluidoSkin>
			<topBarEnabled>false</topBarEnabled>
			<sideBarEnabled>true</sideBarEnabled>
			<googleSearch>
				<siteSearch />
			</googleSearch>
			<gitHub>
				<projectId>taktos/dbflute-hamcrest</projectId>
				<ribbonOrientation>right</ribbonOrientation>
				<ribbonColor>red</ribbonColor>
			</gitHub>
			<twitter>
				<user>taktos9</user>
				<showUser>true</showUser>
				<showFollowers>true</showFollowers>
			</twitter>
		</fluidoSkin>
	</custom>

	<publishDate position="right" />
	<version position="right"/>

	<body>
		<breadcrumbs>
			<item name="DBFlute" href="http://dbflute.seasar.org"/>
		</breadcrumbs>
